{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "guide",
      "aui",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "guide",
      "aui",
      "kebab-case"
    ],
    "component-class-suffix": [
      "Component",
      "Page"
    ]
  }
}
